{
	"servers": {
		"mintlify": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "@open-mcp/mintlify"],
			"env": {
				"MINTLIFY_API_KEY": "${input:mintlify_api_key}",
				"MINTLIFY_PROJECT_ID": "${input:mintlify_project_id}"
			}
		}
	},
	"inputs": [
		{
			"id": "mintlify_api_key",
			"type": "promptString",
			"description": "Mintlify API Key",
			"password": true
		},
		{
			"id": "mintlify_project_id",
			"type": "promptString",
			"description": "Mintlify Project ID",
			"password": false
		}
	]
}